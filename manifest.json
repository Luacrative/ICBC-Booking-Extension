{
    "manifest_version": 3,
    "name": "ICBC Booking Tracker",
    "description": "Tracks ICBC's available road test dates and notifys you whenever there is an appointment that fits your schedule.",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "assets/icon.png"
    },
    "host_permissions": ["https://onlinebusiness.icbc.com/webdeas-ui/home"],
    "permissions": ["storage", "tabs"],
    "background": {
      "service_worker": "scripts/runner.js"
    } 
}  